mixin cards-row(date)
  tr 
    td(class="three_column_184" align="center" style="padding: 0px 12px 0px 12px; font-size:0;")
      <!--[if (gte mso 9)|(IE)]> <table width="100%" cellpadding="0" cellspacing="0" style="border-collapse: collapse;font-family:sans-serif;"> <![endif]-->
      <!--[if (gte mso 9)|(IE)]>  <tr> <![endif]-->
      include ./mixin__card.pug 
      if date.row.length > 0
        each card in date.row
          +card(card)
      else 
        span=date.row.length     
      <!--[if (gte mso 9)|(IE)]> </tr> <![endif]-->
      <!--[if (gte mso 9)|(IE)]> </table> <![endif]-->